{
    "name": "Means",
    "description": "A simple word meaning tool",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/icons/search-12-16.png",
        "32": "/icons/search-12-32.png",
        "48": "/icons/search-12-48.png",
        "128": "/icons/search-12-128.png"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
    ]
}